        h2 Subscribe to Our Newsletter
        p Discounts and special conditions, promotions and bonuses, life hacking promotion and useful services - only for our subscribers.
        .form__input: input(type="email", placeholder="Enter your Email…")
        .form__ctrl: button.btn.white SUBSCRIBE NOW

    .copyright: .container © Copyright 2018 by GetLikes

  .mfp-hide.popup#login
    //(data-scrollbar="horizontal")
    .popup__close(data-close="modal"): svg(): use(xlink:href="#close_icon")
    .popup__header
      svg.text__red: use(xlink:href="#likes_icon")
      h3 200 Instagram Likes
      p Just type your Instagram name and Email
    .popup__form
      input(type="text", placeholder="Instagram Username…")
      input(type="email", placeholder="Your Email…")
      .popup__form--agree
        input#agree(type="checkbox")
        label(for="agree"): svg(): use(xlink:href="#checkbox_icon")
        span I agree with
        a(href="") Terms Of Service
      button.btn.gradient(data-mfp-src="#buy") Next to select posts
  .mfp-hide.popup#buy
    .popup__close(data-close="modal"): svg(): use(xlink:href="#close_icon")
    .popup__header
      svg.text__red: use(xlink:href="#likes_icon")
      h3 200 Instagram Likes for <span class="text__green">$0.69</span>
      p: span.text__red 3 posts selected / 200 likes per post
    .popup__thumbs
      .popup__thumbs--wraper(data-scrollbar)
        ul.thumb__list: -for(i=0;i<16;i++)
          li
            input(type="checkbox", id="thumb_"+i)
            label(for="thumb_"+i)
              img(src="https://picsum.photos/120/120/?image="+i, alt="")
              .likes
                svg(): use(xlink:href="#likes_icon")
                span= 23*(i+i*5)
              .indicator
                svg.l1: use(xlink:href="#likes_icon")
                svg.l2: use(xlink:href="#checkbox_icon")
    .popup__plan
      input#planchange(type="checkbox")
      label(for="planchange")
        span Standart
        span Premium
    .popup__buy: .row
      .col.sm__6: button.btn.outline Add to Card
      .col.sm__6: button.btn.gradient Buy Now
    script(src="js/jquery.min.js")
    script(src="js/main.min.js")
    script(src="js/app.min.js")
